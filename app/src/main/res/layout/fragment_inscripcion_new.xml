                            android:layout_height="128dp"
                            android:scaleType="center"
                            android:src="@drawable/ph_add_image2" />

                    </LinearLayout>

                </LinearLayout>

                <!-- Fin cambios sergio -->


            </LinearLayout>


            <!-- Referencia personal -->
            <TextView
